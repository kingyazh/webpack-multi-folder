{
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "jest": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "experimentalDecorators": 0,
    "linebreak-style": 0,
    "no-extra-boolean-cast": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 1,
    "no-unused-vars": 0,
    "no-var": 0,
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "import/no-unresolved": 0,
    "no-underscore-dangle": [0],
    "prefer-template": 0,
    "object-shorthand": [0],
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "max-len": ["error", {
      "code": 200,
      "tabWidth": 2,
      "comments": 200,
      "ignoreComments": false,
      "ignoreTrailingComments": false,
      "ignoreUrls": true
    }],
    "global-require": 0,
    "no-restricted-syntax": [
      2,
      "LabeledStatement",
      "WithStatement"
    ]
  },
  "globals": {
    "Swiper": true,
    "$": true,
    "jquery": true
  }
}
